{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app/*": ["*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@content/*": ["content/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
     "strictNullChecks": true
  }
}